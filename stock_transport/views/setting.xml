<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="stock_transport_install_res_config_settings_view_form" model="ir.ui.view">
        <field name="name">settings.install.stock.module</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//div" position="before">
                <setting>
                    <div class="app_settings_block" data-string="Install Stock module"
                    id="stock_transport_install_settings"
                    string="Install module"
                    >
                    <div class="row mt16 o_settings_container">
                        <div class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_left_pane" />
                            <div class="o_setting_right_pane">
                                <div class="text-muted content-group mt16">
                                    <label string="Install Stock Transport" for="module_stock_transport"
                                    class="col-lg-3 o_light_label" />
                                    <field name="module_stock_transport"
                                    class="text-center oe_inline" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </setting>                   
            </xpath>
        </field>
    </record>
</odoo>
    